{
  "name": "ue-forged",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm run generate:importmap && pnpm generate-sitemap && turbo run build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation tsx src/seed.ts",
    "migrate:media": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/migrate-media-to-vercel-blob.ts",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --import=tsx/esm\" playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-mongodb": "3.77.0",
    "@payloadcms/email-resend": "^3.77.0",
    "@payloadcms/next": "3.77.0",
    "@payloadcms/richtext-lexical": "3.77.0",
    "@payloadcms/storage-vercel-blob": "3.77.0",
    "@payloadcms/ui": "3.77.0",
    "@tailwindcss/postcss": "^4.2.0",
    "@vercel/blob": "^2.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "framer-motion": "^12.34.3",
    "graphql": "^16.8.1",
    "next": "15.4.11",
    "payload": "3.77.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.71.2",
    "sharp": "0.34.2",
    "tailwindcss": "^4.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "1.58.2",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.19.9",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.11",
    "jsdom": "28.0.0",
    "prettier": "^3.4.2",
    "tsx": "4.21.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
